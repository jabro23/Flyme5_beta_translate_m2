<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ffd9d9d9" android:endColor="#ffd9d9d9" android:angle="270.0" android:centerY="1.0" android:centerColor="#ffd9d9d9" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ffb3b3b3" android:endColor="#ffb3b3b3" android:angle="270.0" android:centerY="1.0" android:centerColor="#ffb3b3b3" />
            </shape>
        </clip>
    </item>
</layer-list>